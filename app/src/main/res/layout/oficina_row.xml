<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/ic_build_black_36dp"
        android:layout_alignBottom="@+id/oficina_nome"
        android:layout_alignEnd="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/oficina_endereco"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView5"
        app:srcCompat="@android:drawable/ic_menu_mylocation" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView6"
        app:srcCompat="@android:drawable/sym_action_call" />

    <TextView
        android:id="@+id/oficina_nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAppearance="@android:style/TextAppearance.Medium"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageView5" />

    <TextView
        android:id="@+id/oficina_endereco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/oficina_nome"
        android:layout_toEndOf="@+id/imageView5"
        android:text="TextView"
        android:textAppearance="@android:style/TextAppearance.Medium" />

    <TextView
        android:id="@+id/oficina_telefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView6"
        android:layout_toEndOf="@+id/imageView6"
        android:text="TextView"
        android:textAppearance="@android:style/TextAppearance.Medium" />

    <RatingBar
        android:id="@+id/oficina_nota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/oficina_telefone"
        android:layout_toEndOf="@+id/textView3"
        android:layout_marginStart="13dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView7"
        android:layout_marginTop="11dp"
        android:text="Nota: "
        android:textColor="@color/colorBlack"
        android:textSize="17dp" />
</RelativeLayout>